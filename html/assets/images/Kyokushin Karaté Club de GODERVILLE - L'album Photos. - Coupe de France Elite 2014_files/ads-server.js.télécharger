		},
		"ffbb-aig" : function( site, params ){
			if ( ! ( /^basket/i ).test( params.type ) || site != "top" ) return this[ "ods" ]( site, params ) ;
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "iframe"
			ad.src = params.base + "/ffbb-aig-2014-06/" + site + "-view.html" ;
			return { slots : [ ad ] } ;
		},
		"ffbb-cdf2016" : function( site, params ){
			if ( ! ( /^basket/i ).test( params.type ) || inArray( params.dep , [ "75", "91", "92", "93", "77", "94", "95", "78" ] ) == -1 || site != "top" ) return this[ "ods" ]( site, params ) ;
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "iframe"
			ad.src = params.base + "/coupe-de-france-2016-02/" + site + "-view.html" ;
			return { slots : [ ad ] } ;
		},
		"ffbb-cnosf-2018" : function( site, params ){
			if ( ! ( /^basket/i ).test( params.type ) || site != "top" ) return this[ "auto" ]( site, params ) ;
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "iframe"
			ad.src = params.base + "/ffbb-cnosf-2018-09/" + site + "-view.html" ;
			return { slots : [ ad ] } ;
		},
		"ffbb-cdf2019" : function( site, params ){
			if ( ! ( /^basket/i ).test( params.type ) || site != "top" ) return this[ "auto" ]( site, params ) ;
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "iframe"
			ad.src = params.base + "/ffbb-coupe-france-2019/" + site + "-view.html" ;
			return { slots : [ ad ] } ;
		}
	}
	function imgsrc( site ){
		var base = ( qlub_ads_slotHTML( { type : "imgwithhttp" } ) == true ) ? ( window.location.protocol + "//core0.quomodo.com" ) : "" ;
		var type = q_spaceparam( "infos" ).type ;
		if ( type && ( /basket/ ).test( type.toLowerCase() ) ) return base + "/images/ads/ods/" + site + "_basket.jpg" ;
		if ( type && ( /foot/ ).test( type.toLowerCase() ) ) return base + "/images/ads/ods/" + site + "_foot.jpg" ;
		return base + "/images/ads/ods/" + site + "_general.jpg" ;
	}
	function getad( site ){
		if ( qlub_ads_slotHTML( { type : "version" } ) != 1 ) return [ "pub" , { slots : [ q_cloneObject( { type : "imglink" , img: imgsrc( site ) , link: "http://lofficieldusport.fr" , target: "_blank" } , dims[ site ] ) ] } ] ;
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "ods", "auto" /* "fb" , "mondialping", "euro-sportring", "euro-sportring", "euro-sportring"*/ ] ;
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "ffbb-aig", "auto" /* "fb" , "mondialping", "euro-sportring", "euro-sportring", "euro-sportring"*/ ] ;
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "ffbb-cdf2016", "ffbb-cdf2016" /* "fb" , "mondialping", "euro-sportring", "euro-sportring", "euro-sportring"*/ ] ;
// 07.04.2016 on passe en 100% Google
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub" ] ;
// 18.07.2016 on passe en 67% Google 33% auto promo Quomodo Pros, 4 campagnes simultanées
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "pub", "pub", "pub", "pub", "pub", "pub", "pub", "auto-pros-ads-07-2016-1", "auto-pros-ads-07-2016-2", "auto-pros-ads-07-2016-3", "auto-pros-ads-07-2016-4" ] ;
// 25.07.2016 on passe à nouveau en 100% Google
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub" ] ;
// 20.09.2016 on active les pubs agence et do-it : 2/3 Google - 1/3 Quomodo et pour Quomodo on fait 1/2 agence (alternance des 2 pubs) et 1/2 do-it
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "pub", "pub", "pub", "pub", "pub", "pub", "pub", "agence-09-2016-1", "agence-09-2016-2", "diy-pros-09-2016", "diy-pros-09-2016" ] ;
//22.09.2016 on ne conserve qu'une des 2 pubs agence qui marche mieux que l'autre, et on passe en 1/3 Google - 1/3 Quomodo doit - 1/3 Quomodo agence
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "agence-09-2016-2", "diy-pros-09-2016" ] ;
//07.11.2016 fin de la promo agence, on passe en 2/3 Google - 1/3 Quomodo doit
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "pub", "diy-pros-09-2016" ] ; // 2019-05-06 : on met les pubs pour l'agence
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "pub", "pub", "pub", "agence-2019-05-shop", "agence-2019-05-vitrine" ] ; // 2020-01-29 : on ajoute la publicité site cle en main
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "pub", "pub", "agence-2020-01-cle", "agence-2019-05-shop", "agence-2019-05-vitrine" ] ; // 2021-08-24 : on met les publicités EVODE
		//var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "evode-boutique-2021-08", "evode-metier-artisan-2021-08", "evode-metier-coach-2021-08", "evode-metier-commercant-2021-08", "evode-metier-createur-2021-08", "evode-metier-freelance-2021-08", "evode-vitrine-2021-08" ] ; // 2021-08-24 : finalement on ne met que les publicités EVODE
		var ads = ( window.quomodoclient && quomodoclient.ads && quomodoclient.ads.qIDS ) || [ "pub", "pub", "pub", "pub", "pub", "pub", "pub", "evode-boutique-2021-08", "evode-metier-artisan-2021-08", "evode-metier-coach-2021-08", "evode-metier-commercant-2021-08", "evode-metier-createur-2021-08", "evode-metier-freelance-2021-08", "evode-vitrine-2021-08" ] ; // 2021-08-31 : finalement, nouveau test, on remet les pubs en mode non personnalisé
		var params = {
			"base": window.location.protocol + "//ads" + ( ( isrealserver() ) ? "" : "1" ) + ".quomodo.com",
			"id": q_spaceparam( "id" ),
			"use": ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).subscribe_use : "other",
			"type": ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).type : "other",
			"dep": ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).department : "",
			"zip": ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).zipcode : ""
		} ;
		if ( window.qsn_responsive_mod && qsn_responsive_mod() == "xs" ) ads = [ "pub" ] ;
		try {
			if ( q_pagemode() == "edit" ) ads = [ "edit" ] ;
		} catch( err ) {}
		//if ( params.id == "55F85F19-7BCA-4CEC-A215-3405A2679A2E" ) ads = [ "diy-pros-09-2016" ] ;
		/*
		if ( ( /^basket/i ).test( params.type ) ) {
			ads = [ "ffbb-cnosf-2018" ] ;
			if ( window.qsn_responsive_mod && qsn_responsive_mod() == "xs" ) ads = [ "auto" ] ;
		}
		*/
/*
// 2019-04-02, nb affichages atteint, on arrête la campagne
		if ( ( /^basket/i ).test( params.type ) ) {
			//ads = [ "ffbb-cdf2019", "ffbb-cdf2019", "pub" ] ;
			// 2019-02-19, on passe à 100%
			ads = [ "ffbb-cdf2019" ] ;
			if ( window.qsn_responsive_mod && qsn_responsive_mod() == "xs" ) ads = [ "auto" ] ;
		}
*/
		if ( ( /^[0-9]{5}$/ ).test( params.zip ) ) params.dep = params.zip.substring( 0, ( ( /^9[7-9]/ ).test( params.zip ) ) ? 3 : 2 ) ;
		var displayedads = [] ;
		for ( var i = 0 ; i < ads.length ; i++ ) {
			var ad = ads_def[ ads[ i ] ] && ads_def[ ads[ i ] ]( site, params, window.qsn_responsive_mod && qsn_responsive_mod() ) ;
			if ( ad ) displayedads.push( ad ) ;
		}
		return displayedads ;
	}
	selected_ads[ site ] = getad( site ) ;
	return selected_ads[ site ] ;
}

var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];

function qlub_init_pubtopads( siteads ) {
	if ( window.qlub_ads_cmd ) {
		$( "#qlub_topads-gpt" ).remove() ;
		var $target = qlub_addTarget( "qlub_topads" ) ;
		$target.append( '<div id="qlub_topads-gpt" style="width:728px; height:90px;"></div>' ) ;
		try {
			googletag.cmd.push(function() {
				googletag.display( "qlub_topads-gpt" ) ;
			} );
		} catch( err ) {}
		return true ;
	}
	document.write( '<div id="qlub_topads-gpt" style="width:728px; height:90px;"><script type="text/javascript">\n' + '//<![CDATA[\n' + 'try { googletag.cmd.push(function() { googletag.display( "qlub_topads-gpt" ); }); } catch( err ) {}\n' + '//]]>\n' + '</script></div>' ) ;
	return true ;
}

function qlub_init_pubbottomads( siteads ) {
	if ( window.qlub_ads_cmd ) {
		$( "#qlub_bottomads-gpt" ).remove() ;
		var $target = qlub_addTarget( "qlub_bottomads" ) ;
		$target.append( '<div id="qlub_bottomads-gpt" style="width:468px; height:60px;"></div>' ) ;
		try {
			googletag.cmd.push(function() {
				googletag.display( "qlub_bottomads-gpt" ) ;
			} );
		} catch( err ) {}
		return true ;
	}
	document.write( '<div id="qlub_bottomads-gpt" style="width:468px; height:60px;"><script type="text/javascript">\n' + '//<![CDATA[\n' + 'try { googletag.cmd.push(function() { googletag.display( "qlub_bottomads-gpt" ); }); } catch( err ) {}\n' + '//]]>\n' + '</script></div>' ) ;
	return true ;
}

function qlub_init_pubrightads( siteads ) {
	if ( ! window.pages_adsdimensions ) return false ;
	var x = pages_adsdimensions() ;
	var w = x.width - x.paddingleft - x.paddingright ;
	var td = document.getElementById( "qlub_ads_addright_elem" ) ;
	if ( td ) {
		td.style.width = w + "px" ;
		td.style.padding = "6px " + x.paddingright + "px 6px " + x.paddingleft + "px" ;
	}
	if ( window.qlub_ads_cmd ) {
		$( "#qlub_rightads-gpt" ).remove() ;
		var $target = qlub_addTarget( "qlub_rightads" ) ;
		if ( $target.length == 0 ) $target = qlub_addTarget( "qlub_ads_addright_elem" ) ;
		$target.append( '<div id="qlub_rightads-gpt" style="width:120px; height:600px;"></div>' ) ;
		try {
			googletag.cmd.push(function() {
				googletag.display( "qlub_rightads-gpt" ) ;
			} );
		} catch( err ) {}
		return true ;
	}
	//var slot = (  q_currsvcishome() ) ? "Accueil_HautPage_Skycraper_120x600" : "Pages_HautPage_Skycraper_120x600";
	//if ( ! window.q_ads_version ) slot = "ad_right" ;
	document.write( '<div id="qlub_rightads-gpt" style="width:120px; height:600px;"><script type="text/javascript">\n' + '//<![CDATA[\n' + 'try { googletag.cmd.push(function() { googletag.display( "qlub_rightads-gpt" ); }); } catch( err ) {}\n' + '//]]>\n' + '</script></div>' ) ;
	return true ;
}

