function qlub_ads_head_loadads() {
	if ( q_pagemode() == "makeweb" || q_pagemode() == "edit" ) return ;
	if ( qlub_noads_forie() ) return ;
	var b = ( qlub_ads_checkadd( "top" ) == "pub" ) || ( qlub_ads_checkadd( "right" ) == "pub" ) || ( qlub_ads_checkadd( "bottom" ) == "pub" ) ;
	if ( ! b ) return ;// on n'est pas en mode pub on ne charge rien de plus pour Publisher
	quomodo.events.addListener( "finish_init.page" , function( eventDesc, eventName ){
		var b = false ;
		if ( ! b ) {
			var sites = [ "top", "right", "bottom" ] ;
			for ( var i = 0 ; i < sites.length ; i++ ) {
				var ads = ads_getad( sites[ i ] ) ;
				if ( ads ) {
					for ( var j = 0 ; j < ads.length ; j++ ) {
						if ( ads[ j ] == "pub" ) {
							b = true ;
							break ;
						}
					}
				}
				if ( b ) break ;
			}
		}
/*
		b = b || ( ads_getad( "top" ) == "pub" ) ;
		b = b || ( ads_getad( "right" ) == "pub" ) ;
		b = b || ( ads_getad( "bottom" ) == "pub" ) ;
*/
/*
		var accept_cookies = getcookie( "accept-cookies" ) ;
		var cookies_enabled = ( window.cookiesEnabled || function() {
			setcookie( "test-cookies-enabled", "1", 1, true ) ;
			var res = getcookie( "test-cookies-enabled" ) == "1" ;
			deletecookie( "test-cookies-enabled", true ) ;
			return res ;
		} )() ;
		var cookies_accepted = ( function() {
			if ( accept_cookies == "0" ) return false ;
			if ( ! cookies_enabled ) return true ;
			if ( accept_cookies != "1" ) {
				cookiesConfirm() ;
				return false ;
			}
			return true ;
		} )() ;
*/
		if ( ! b ) return ;
		
		if ( ! document.getElementById( "gpt-googletagservices" ) ) {
			var gads = document.createElement("script");
			gads.async = true;
			gads.type = "text/javascript";
			var useSSL = "https:" == document.location.protocol;
			gads.src = (useSSL ? "https:" : "http:") + "//www.googletagservices.com/tag/js/gpt.js";
			var node = document.getElementsByTagName("script")[0];
			node.parentNode.insertBefore(gads, node);
			gads.id = "gpt-googletagservices" ;
		}
	} ) ;

	googletag.cmd.push( function() {
		googletag.pubads().setRequestNonPersonalizedAds(1) ;
		var mappingTop = googletag.sizeMapping().
			addSize( [769, 0], [728, 90] ).
			addSize( [0, 0], [320, 50] ).
			build() ;
		var mappingBottom = googletag.sizeMapping().
			addSize( [769, 0], [468, 60] ).
			addSize( [0, 0], [320, 50] ).
			build() ;
		var mappingRight = googletag.sizeMapping().
			addSize( [769, 0], [120, 600] ).
			addSize( [0, 0], [] ).
			build() ;
		if ( q_currsvcishome() ) {
			googletag.defineSlot( "/1251785/Accueil_BasPage_Centre_468x60", [468, 60], "qlub_bottomads-gpt" ).defineSizeMapping( mappingBottom ).addService(googletag.pubads());
			googletag.defineSlot( "/1251785/Accueil_HautPage_Centre_728x90", [728, 90], "qlub_topads-gpt" ).defineSizeMapping( mappingTop ).addService(googletag.pubads());
			googletag.defineSlot( "/1251785/Accueil_HautPage_Skycraper_120x600", [120, 600], "qlub_rightads-gpt" ).defineSizeMapping( mappingRight ).addService(googletag.pubads());
		} else {
			googletag.defineSlot( "/1251785/Pages_BasPage_Centre_468x60", [468, 60], "qlub_bottomads-gpt" ).defineSizeMapping( mappingBottom ).addService(googletag.pubads());
			googletag.defineSlot( "/1251785/Pages_HautPage_Centre_728x90", [728, 90], "qlub_topads-gpt" ).defineSizeMapping( mappingTop).addService(googletag.pubads());
			googletag.defineSlot( "/1251785/Pages_HautPage_Skycraper_120x600", [120, 600], "qlub_rightads-gpt" ).defineSizeMapping( mappingRight ).addService(googletag.pubads());
		}
		googletag.pubads().enableSingleRequest();
		googletag.enableServices();
	});
	var bgcolor = ( window.club_siteelemscolor && club_siteelemscolor.cfs ) ? club_siteelemscolor.cfs : "rgb(255,255,255)"; 
	bgcolor = rgb2hex( bgcolor ).replace( "#", "" );
	googletag.cmd.push( function() {
		googletag.pubads().set("adsense_background_color", bgcolor );
	} );
	googletag.cmd.push( function() {
		googletag.pubads().setTargeting( "Use", ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).subscribe_use : "other" ) ;
		googletag.pubads().setTargeting( "Sport", ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).type : "other" ) ;
	});
}

var ads_server_js_loaded = true ;
if ( window.qdyn_loaded ) qdyn_loaded ( "ads_server_js_loaded" ) ;