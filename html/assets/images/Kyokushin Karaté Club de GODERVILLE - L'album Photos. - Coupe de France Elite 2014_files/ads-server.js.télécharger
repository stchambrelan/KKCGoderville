function ads_getcookiesads(){
	var host = ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).host : null ;
//	if ( host == "www.otcf.fr" ) return [ "accropizz/ad0" ] ;
//	if ( host == "www.eso-tennis-de-table.fr" ) return [ "accropizz/ad0" ] ;
//	if ( host == "www.rsstcyrbasket.fr" ) return [ "accropizz/ad0" ] ;
//	if ( host == "www.pionniersdetouraine.fr" ) return [ "accropizz/ad0" ] ;
//	if ( host == "www.pllltoursbasket.com" ) return [ "accropizz/ad0" ] ;
	var privatedomain = ( q_spaceparam( "infos" ) ) ? q_spaceparam( "infos" ).privatedomain : null ;
//	if ( privatedomain == "otcf" ) return [ "accropizz/ad0" ] ;
//	if ( privatedomain == "bctn" ) return [ "accropizz/ad0" ] ;
//	if ( privatedomain == "summercampbasketball" ) return [ "accropizz/ad0" ] ;
//	if ( privatedomain == "avenir-fc-tours" ) return [ "accropizz/ad0" ] ;
//	if ( privatedomain == "bbc-tours-tennisdetable" ) return [ "accropizz/ad0" ] ;
	return null ;
}
function q_acceptCookies( b ) {
	if ( b == "later" ) {
		q_acceptCookiesLoaded = false ;
		$( "#accept-cookies" ).remove() ;
		$( "body" ).css( "margin-top", "auto" ).removeAttr( "cookie-margin-top" ) ;
		window.quomodo && quomodo.events.trigger( "topads.loaded" ) ;
		return ;
	}
	setcookie( "accept-cookies", b ? "1" : "0", 365, true ) ;
	q_acceptCookiesLoaded = false ;
	$( "#accept-cookies" ).remove() ;
	$( "body" ).css( "margin-top", "auto" ).removeAttr( "cookie-margin-top" ) ;
	window.quomodo && quomodo.events.trigger( "topads.loaded" ) ;
}
function q_cookiesMoreInfos( hide ) {
	if ( hide ) {
		$( "#accept-cookies-step2" ).hide() ;
		$( "#accept-cookies-step1" ).show() ;
		return ;
	}
	var h = $( window ).height() - parseInt( $( "#accept-cookies" ).css( "padding-top" ), 10 ) - parseInt( $( "#accept-cookies" ).css( "padding-bottom" ), 10 ) ;
	if ( $( "#accept-cookies-step2" ).data( "prepared" ) ) {
		$( "#accept-cookies-step2" ).show() ;
		$( "#accept-cookies-step2 .content" ).height( h ) ;
		$( "#accept-cookies-step1" ).hide() ;
		return ;
	}
	q_fsm_wait() ;
	q_include( { "target": "accept-cookies-step2", urls : [ "/clients/qlubads0910/accept_cookies_" + q_lang + ".html" ] }, function() {
		q_fsm_delete() ;
		$( "#accept-cookies-step2" ).show() ;
		$( "#accept-cookies-step2 .content" ).height( h ) ;
		$( "#accept-cookies-step1" ).hide() ;
	} ) ;
}

var q_acceptCookiesLoaded ;
function cookiesConfirm() {
	if ( window != parent.window ) return ; // popup
	if ( q_acceptCookiesLoaded ) return ;
	q_acceptCookiesLoaded = true ;
	quomodo.events.addListener( "finish_init.page" , function( eventDesc, eventName ){
		if ( $( "#accept-cookies" ).length > 0 ) return ;
		var dict = {
			"fr": {
				"prompt": "En poursuivant votre navigation sur ce site, vous acceptez l’utilisation de cookies ou autres traceurs pour adapter à vos centres d'intérêt les publicités qui financent ce site.",
				"ok": "OK j'accepte",
				"more": "En savoir plus",
				"later": "Plus tard"
			},
			"en": {
				"prompt": "By continuing your browsing on this site, you accept the use of cookies or other tracers to adapt to your interests the advertisements that finance this site.",
				"ok": "OK I agree",
				"more": "Learn more",
				"later": "Later"
			}
		} ;
		$( "body" ).append( '<div id="accept-cookies" style="position : fixed ; width: 100%; z-index: 5; font-size: 11px; color: #000; top: 0px; left: 0px; font-family: Lato, sans-serif; background-color: #f5f5f5; border-bottom: 1px solid #ddd; padding: 4px 18px ; box-sizing: border-box ; text-align: justify"><div id="accept-cookies-step1" style="text-align: center"><div>' + dict[ q_lang ][ "prompt" ] + '</div><span onclick="q_acceptCookies( \'later\' ) ; return false" style="float: right ; color: #1479FC ; cursor: pointer">' + dict[ q_lang ][ "later" ] + '</span><span onclick="q_cookiesMoreInfos() ; return false" style="float: right ; color: #1479FC ; cursor: pointer ; margin-right: 18px">' + dict[ q_lang ][ "more" ] + '</span><span onclick="q_acceptCookies( true ) ; return false" style="float: right ; color: #1479FC ; cursor: pointer ; margin-right: 18px ; font-weight: bold">' + dict[ q_lang ][ "ok" ] + '</span></div><div id="accept-cookies-step2" style="font-size: 14px ; display: none"></div></div>' ).css( "margin-top", "35px" ).attr( "cookie-margin-top", "35" ) ;
		window.quomodo && quomodo.events.trigger( "topads.loaded" ) ;
	} ) ;
}

var selected_ads = {} ;
function ads_getad( site ) {
	if ( selected_ads[ site ] ) return selected_ads[ site ] ;
	var dims = { "top" : { width : "728", height : "90" }, "top_xs" : { width : "320", height : "50" }, "right" : { width : "120", height : "600" }, "bottom" : { width : "468", height : "60" }, "bottom_xs" : { width : "320", height : "50" } } ;
	function inArray( item, list ){
		if ( list.indexOf ) return list.indexOf( item ) ;
		for ( var i = 0 ; i < list.length ; i++ ){
			if ( item == list[ i ] ) return i ;
		}
		return -1 ;
	}
	var accept_cookies = getcookie( "accept-cookies" ) ;
	var cookies_enabled = ( window.cookiesEnabled || function() {
		setcookie( "test-cookies-enabled", "1", 1, true ) ;
		var res = getcookie( "test-cookies-enabled" ) == "1" ;
		deletecookie( "test-cookies-enabled", true ) ;
		return res ;
	} )() ;
	var ads_def = {
		"pub" : function( site, params ){
			return "pub" ; // 2021-08-31 : on règle les annonces de Google en mode non personnalisé donc pas de consentement
			if ( accept_cookies == "0" ) return null ;
			if ( ! cookies_enabled ) return "pub" ;
			if ( accept_cookies != "1" ) {
				cookiesConfirm() ;
				return null ;
			}
			return "pub" ;
		},
		"edit" : function( site, params ){
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "editad" ;
			ad.site = site ;
			return { slots : [ ad ] } ;
		},
		"ods" : function( site, params ){
			if ( params.use != "sport" ) return null ;
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "iframe" ;
			if ( ( /foot/ ).test( params.type.toLowerCase() ) ) ad.src = params.base + "/ods/" + site + "_foot-view.html" ;
			else if ( ( /basket/ ).test( params.type.toLowerCase() ) ) ad.src = params.base + "/ods/" + site + "_basket-view.html" ;
			else ad.src = params.base + "/ods/" + site + "_general-view.html" ;
			return { slots : [ ad ] } ;
		},
		"auto" : function( site, params ){
			if ( params.use != "sport" ) return null ;
			if ( site != "top" ) return ads_def [ "ods" ] ( site, params ) ;
			var imgurl = autotypedef [ params.type ] && autotypedef [ params.type ].imgurl ;
			var referer = autotypedef [ params.type ] && autotypedef [ params.type ].referer ;			
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "iframe" ;
			ad.src = params.base + "/auto-06-2014/" + site + "_auto-view.html" ;
			var sep = "?" ;
			if ( imgurl ) ad.src += "?i=" + imgurl ;
			if ( imgurl ) sep = "&" ;
			if ( referer ) ad.src += sep + "refererpage=" + referer ;
			return { slots : [ ad ] } ;
		},
		"auto-pros-ads-07-2016-1" : function( site, params ){
			if ( site == "bottom" ) return this[ "pub" ]( site, params ) ;
			var ad = q_cloneObject( dims[ site ] ) ;
			ad.type = "iframe"
			ad.src = params.base + "/auto-pros-ads-07-2016-1/" + site + "-view.html" ;
			return { slots : [ ad ] } ;
		},
		"auto-pros-ads-07-2016-2" : function( site, params ){
			if ( site == "bottom" ) return this[ "pub" ]( site, params ) ;
			var ad = q_cloneObject( dims[ site ] ) ;
