var sortname={};function toXML(id,key,json){var tmp=key.split(":");return '<'+tmp[0]+(tmp[1]?(' id="'+id+'" kind="'+tmp[1]+'"'):(' value="'+id+'"'))+'>'+q_toxmltags(json)+'</'+tmp[0]+'>';}
