function isfirefox(){return(navigator)&&(navigator.userAgent)&&(/FireFox/i.test(navigator.userAgent));}
function issafari(){return(navigator)&&(navigator.appVersion)&&(/AppleWebKit/.test(navigator.appVersion));}
