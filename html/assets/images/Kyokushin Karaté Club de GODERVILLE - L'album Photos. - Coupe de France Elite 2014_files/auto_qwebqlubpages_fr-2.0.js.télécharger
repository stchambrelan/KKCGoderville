o[k]=tmp;if(width_def[format_col(o.l,o.r-1)]>=w)break;}
return o;}
function setMarginFlag(lcol,rcol){if(json.common.m!=-1)return;var f=0;if(!lcol||!fullcol_def[lcol].l)f=f+1;if(!lcol||!fullcol_def[rcol].r)f=f+2;json.common.m=f;}
