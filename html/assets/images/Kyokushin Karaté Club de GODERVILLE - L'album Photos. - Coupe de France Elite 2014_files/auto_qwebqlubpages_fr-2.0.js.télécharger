function specialProd(x0,y0,mode,coeff,excludeds){var ks=["serif","sans-serif","rond","carr","gras","bold0","fin","large","spacing0","condens"];if(mode==true){var p=specialProd(x0,y0,"font")+(coeff||1)*specialProd(x0,y0,"other");for(var i=0;i<y0.length;i++){if($.inArray(y0[i],excludeds)!=-1)p-=2;}
return p;}
var xx=[],yy=[],x=[],y=[];for(var i=0;i<x0.length;i++){var k=x0[i].split(":")[0];if(mode=="font"){if($.inArray(k,ks)!=-1){x.push(x0[i]);xx.push(k);}}else if(mode=="other"){if($.inArray(k,ks)==-1){x.push(x0[i]);xx.push(k);}}else{x.push(x0[i]);xx.push(k);}}
