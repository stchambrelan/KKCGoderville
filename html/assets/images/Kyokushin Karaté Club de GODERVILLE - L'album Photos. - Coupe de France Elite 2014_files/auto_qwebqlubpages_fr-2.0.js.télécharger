function qsn_width_xs(id){var r=qsn_thenotes[id];var bDup=/-dup/.test(id);var bDup_xs=/-dup-xs/.test(id);if(!r&&bDup){id=id.replace(/-dup.*/,"");r=qsn_thenotes[id];}
