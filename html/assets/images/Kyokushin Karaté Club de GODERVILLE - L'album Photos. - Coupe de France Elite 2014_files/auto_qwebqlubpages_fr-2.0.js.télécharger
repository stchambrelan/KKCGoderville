align_list.sort(function(m1,m2){var v1=m1[1];var v2=m2[1];var id1=m1[0];var id2=m2[0];if(v1==v2){if((/note_/).test(id1)&&(/note_/).test(id2)){if((/note_([0-9]*)/).exec(id1)[1]<(/note_([0-9]*)/).exec(id2)[1])return 1;}
return-1;}
