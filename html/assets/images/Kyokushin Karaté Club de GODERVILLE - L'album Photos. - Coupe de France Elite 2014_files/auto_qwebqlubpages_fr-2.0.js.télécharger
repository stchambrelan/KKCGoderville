function poll2_fixvote(x){if(x.vote&&x.vote.options){var options={};for(var k in x.vote.options){if(!/^\d+$/.test(k)&&!/-\d+$/.test(k))continue;var optionid=k.match(/(\d+)$/)[1];options[optionid]=x.vote.options[k];}
