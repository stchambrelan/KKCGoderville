function encode_col(id,fittocolumn){var div=ge(id);if(qsn_thenotes[div.id].header&&qsn_thenotes[div.id].header.display=="fold"&&$(".tablenotecontent",div).css("display")=="none"){$(div).css("min-width",qsn_minwidth(div)+"px");}
