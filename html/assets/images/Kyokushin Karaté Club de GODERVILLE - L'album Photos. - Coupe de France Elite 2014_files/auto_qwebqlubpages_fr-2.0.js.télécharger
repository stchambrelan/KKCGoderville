$(td).html(parseInt(dayid.split("_")[2],10)).attr({"dayid":dayid,"currmonth":(iscurrmonth)?"true":"false","currday":(istoday)?"true":"false","hasevent":hasevent,"currmonth-hasevent":(hasevent!="false"&&iscurrmonth)?"true":"false","title":""});}
